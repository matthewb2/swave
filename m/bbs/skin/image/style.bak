#navigation {
background:url(/images/navbar.png);
width:850px;
height:25px;
padding: 12px 0;
font:90% Tahoma;
color:#fff;

letter-spacing: 1px;
text-transform:uppercase;

-moz-border-radius-topright: 5px;
border-top-right-radius: 5px;
	
-moz-border-radius-topleft: 5px;
border-top-left-radius: 5px;

}


#nav ul{
margin:0px 0;
z-index: 99;
}

#nav a {
padding:10px;
text-decoration:none;
color: #bdbdc1

}

#nav a:hover {

background:#000;
color:#fff;

-moz-border-radius-bottomright: 5px;
border-bottom-right-radius: 5px;
	
-moz-border-radius-bottomleft: 5px;
border-bottom-left-radius: 5px;

-moz-border-radius-topright: 5px;
border-top-right-radius: 5px;
	
-moz-border-radius-bottomleft: 5px;
border-top-left-radius: 5px;

}

#nav a.active {
background:#000;
color:#fff;

-moz-border-radius-bottomright: 5px;
border-bottom-right-radius: 5px;
	
-moz-border-radius-bottomleft: 5px;
border-bottom-left-radius: 5px;

-moz-border-radius-topright: 5px;
border-top-right-radius: 5px;
	
-moz-border-radius-bottomleft: 5px;
border-top-left-radius: 5px;

}

#nav li
	{	
	list-style:none;
	float: left;
	background: #293453;	
	}

#nav li a
	{	text-decoration: none;
		width: 70px;
		white-space: nowrap }

#nav li ul
		{	margin: 0;
			padding: 0;
			position: absolute;
			visibility: hidden; }
		
#nav li ul li
		{	float: none;
			padding: 5px;
			display: block;
			background: #293453;
		}
			
#nav li ul li a
			{ width: auto; }
			
#nav li ul li a:hover
			{background: #2e2e2e
			}


