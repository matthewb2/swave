#popup_layer {   
		position:absolute; bottom:50px; right:150px;
		display:block;
		width:350px; 
		border:0px solid grey; 
		padding:5px;
		opacity: 0;   
		transition: opacity 3s ease;
		}
	#show {
		position:relative;
		display:block;
		width:350px; 
		border:1px solid grey; 
		padding:5px;
		opacity: 1;   
		transition: opacity 3s ease;
		background:#FBFAF7;
		}
	#hide {
		width:350px; 
		border:1px solid grey; 
		padding:5px;
		opacity: 0;   
		transition: opacity 3s ease;
		background:#FBFAF7;
		}
	
	#show2 {
		position:relative;
		display:block;
		width:350px; 
		border:1px solid grey; 
		padding:5px;
		opacity: 1;   
		transition: opacity 3s ease;
		background:#FBFAF7;
		}
	#hide2 {
		width:350px; 
		border:1px solid grey; 
		padding:5px;
		opacity: 0;   
		transition: opacity 3s ease;
		background:#FBFAF7;
		}
